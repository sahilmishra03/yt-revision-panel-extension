{
  "manifest_version": 3,
  "name": "YT Revision Panel",
  "version": "1.0",
  "description": "Mark and revise YouTube playlist videos with timestamps",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
